{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "playcardsys-terminal",
    "version": "0.1.6"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "br.com.playcardsys.terminal",
      "targets": [
        "deb"
      ],
      "category": "Utility",
      "shortDescription": "PlayCardSys Terminal"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false,
      "endpoints": [
        "https://gist.githubusercontent.com/DanielScapulatempo/af3319fcadf0b62963e7bfa8865a6a63/raw"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDc3MEU2MERDMTE3RENERDAKUldUUXpYMFIzR0FPZDNYMWk3cW1ibWxRYTlwUCtKbFQrL0dySmpGWGg3ZStLekRYWGpuYk1GVzcK"
    },
    "windows": [
      {
        "fullscreen": true,
        "resizable": true,
        "title": "Terminal",
        "width": 800,
        "height": 600,
        "center": true,
        "focus": true,
        "decorations": false,
        "alwaysOnTop": false
      }
    ]
  }
}
